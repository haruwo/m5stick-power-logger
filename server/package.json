{
  "name": "m5stick-power-logger-server",
  "version": "1.0.0",
  "description": "Server setup for M5StickC Power Logger",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "dev": "docker-compose -f docker-compose.dev.yml up",
    "logs": "docker-compose logs -f",
    "build": "docker-compose build",
    "test:e2e": "cd e2e && npm test",
    "test:e2e:docker": "cd e2e && npm run test:docker",
    "test:e2e:setup": "cd e2e && npm install && npm run setup"
  },
  "keywords": [
    "m5stick",
    "power-logger",
    "docker"
  ],
  "author": "M5StickC Power Logger",
  "license": "MIT"
}